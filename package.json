{
  "name": "poc-backend",
  "version": "1.0.0",
  "description": "poc-backend API",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "start": "serverless offline",
    "test": "nyc mocha",
    "predeploy": "rm -rf build/",
    "deploy": "sls deploy -v",
    "prepush": "yarn test && yarn lint"
  },
  "nyc": {
    "function": "70",
    "branches": "100",
    "check-coverage": "true",
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "test/**"
    ],
    "extension": [
      ".ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/allied-modalgr/serverless-template.git"
  },
  "keywords": [
    "API",
    "Serverless",
    "Template"
  ],
  "author": "ModalGR team",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/allied-modalgr/serverless-template/issues"
  },
  "homepage": "https://gitlab.com/allied-modalgr/serverless-template#readme",
  "dependencies": {
    "@types/mysql": "^2.15.7",
    "aws-xray-sdk": "2.2.0",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "express-fileupload": "^1.1.6-alpha.6",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.5",
    "mysql": "^2.17.1",
    "mysql2": "^1.7.0",
    "serverless-domain-manager": "^3.3.0",
    "serverless-http": "1.9.1",
    "swagger-ui-express": "4.0.2",
    "winston": "3.2.1",
    "xlsx": "^0.15.1"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/chai-http": "4.2.0",
    "@types/compression": "0.0.36",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.16.1",
    "@types/mocha": "5.2.6",
    "@types/request": "2.48.1",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "chai": "4.2.0",
